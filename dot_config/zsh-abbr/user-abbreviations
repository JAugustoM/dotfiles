abbr "bearo"="bear --output build/compile_commands.json --"
abbr "brewup"="brew update ; brew upgrade"
abbr "cninja"="cmake -G Ninja -S . -B build/"
abbr "ezat"="eza -a -l -T -L 1% --git"
abbr "gm"="git commit -m \"%\""
abbr "s"="sudo"
abbr "sdi"="sudo dnf install"
abbr "sdr"="sudo dnf remove"
abbr "nelg"="sudo nix-env --profile /nix/var/nix/profiles/system --list-generations"
abbr "nedg"="sudo nix-env --profile /nix/var/nix/profiles/system --delete-generations"
abbr "ncg"="sudo nix-collect-garbage"
